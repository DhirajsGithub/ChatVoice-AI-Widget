{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "routes": [
      {
        "src": "/agent-widget.js",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Content-Type": "application/javascript",
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/widget/(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Content-Type": "application/javascript",
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }