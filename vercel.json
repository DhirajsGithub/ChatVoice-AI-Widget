{
    "version": 1,
    "builds": [
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Content-Type": "application/javascript"
        },
        "dest": "/dist/$1"
      }
    ]
  }
  